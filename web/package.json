    "concurrently": "^8.2.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "graphql": "^16.8.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
