    "@react-aria/ssr": "^3.9.1",
    "@react-icons/all-files": "^4.1.0",
    "@tanstack/react-query": "^5.17.15",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.18.0",
    "next": "14.1.0",
    "next-intl": "3.4.5",
    "next-themes": "^0.2.1",
    "psl": "^1.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spring": "^9.7.3",
    "valtio": "^1.13.0"
  },
  "devDependencies": {
