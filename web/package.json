{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npx graphql-code-generator --watch \"src/documents/**/*.ts\"\"",
    "dev2": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore -w ./src",
    "prettier": "prettier -c ./src",
    "typecheck": "tsc --noEmit --incremental false",
    "fix": "npm run typecheck && npm run lint && npm run format && npm run gen",
    "gen": "npx graphql-code-generator"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@react-icons/all-files": "^4.1.0",
    "@tanstack/react-query": "^5.13.4",
    "dayjs": "^1.11.10",
    "react": "^18",
    "next-intl": "3.3.2",
    "react-spring": "^9.7.3",
    "react-dom": "^18",
    "next": "14.0.4",
    "valtio": "^1.12.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.3.0",
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8",
    "tailwindcss": "^3.3.6",
    "graphql": "^16.8.1",
    "eslint": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "eslint-config-next": "14.0.4"
  }
}